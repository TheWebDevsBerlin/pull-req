{
  "name": "pull-req",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "DEBUG=pull-req:* nodemon ./bin/www",
    "client": "cd client && npm start",
    "build": "cd client && react-scripts build",
    "dev-windows": "nodemon ./bin/www",
    "heroku-pre-build": ""
  },
  "dependencies": {
    "@octokit/rest": "^18.0.6",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "chalk": "^4.1.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "hbs": "^4.0.1",
    "mongoose": "^5.2.10",
    "morgan": "^1.9.0",
    "node-sass-middleware": "^0.11.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "proxy-addr": "^2.0.6",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^1.12.1"
  }
}
